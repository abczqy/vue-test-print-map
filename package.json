{
  "name": "test1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service --fix lint"
  },
  "dependencies": {
    "ant-design-vue": "^1.5.3",
    "core-js": "^3.6.4",
    "less": "^3.11.1",
    "vue": "^2.6.11",
    "vue-print-nb": "^1.5.0",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "indent": [
        2,
        2
      ],
      "no-mixed-spaces-and-tabs": [
        2,
        false
      ],
      "no-multi-spaces": 1,
      "no-multiple-empty-lines": [
        1,
        {
          "max": 2
        }
      ],
      "no-spaced-func": 2,
      "no-var": 1,
      "comma-dangle": [
        2,
        "never"
      ],
      "eqeqeq": 2,
      "comma-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "computed-property-spacing": [
        "error",
        "never"
      ],
      "vue/no-parsing-error": [2, { "x-invalid-end-tag": false }]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
